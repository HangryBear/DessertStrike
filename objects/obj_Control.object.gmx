            <kind>1</kind>
            <string>/// Code to setup the game including all the objects in the level

global.player_dir = image_angle // init glob var
global.cakes = 5 // init glob var

screenshot = 0 // var to handle screenshot index

randomize(); // ensure the map and all other random events are fully randomised - can change this for defined seeds in order to generate proceduralised levels


// create random oases across the level so we have some variety
for (i=0; i&lt;random_range(10,20); i+=1)
{
    inst = instance_create(random_range(16,room_width-16),random_range(16,room_height-64) , choose(obj_Island2,obj_Island3))
    inst.image_angle = random_range(0,145)
    scaler = random_range(0.4,1)
    inst.image_xscale = scaler
    inst.image_yscale = scaler
    
};

// create random palm trees across the level so we have some variety
for (i=0; i&lt;random_range(10,20); i+=1)
{
    inst = instance_create(random_range(16,room_width-16),random_range(16,room_height-64) , obj_Palm1)
    inst.image_xscale = choose(1,-1)
};



// create random desserts across the level for collection
for (i=0; i&lt;global.cakes; i+=1)
{
    inst = instance_create(random_range(16,room_width-16),random_range(16,room_height-64) , choose(obj_Cake1,obj_Cake2))
    inst.image_xscale = choose(1,-1)
};



// init player obj and slow animation rate
with (instance_create(room_width/2, room_height-150, obj_MyPlane))    {    image_speed = 0.4;    }

// From Heartbeast book, adjust noise so slightly different each time.
audio_em = audio_emitter_create()
//audio_emitter_pitch(audio_em,random_range(.5,1.5))
//audio_emitter_gain(audio_em,random_range(.7,1))
audio_emitter_gain(audio_em,0.3)
audio_play_sound_on(audio_em,snd_Hover1,true,10)


// Create alarm for spawning baddies
alarm[10] = room_speed * 2;
</string>
          </argument>
        </arguments>
      </action>
