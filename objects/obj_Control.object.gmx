            <string>/// Level gen code &amp; global vars

global.player_dir = image_angle // init glob var
global.cakes = 5 // init glob var
global.game_won = false // stop play once all the cakes collected
global.iAlpha = 0 // for winning screen
global.enemyRadar = false // see enemy view rings, for debug only



screenshot = 0 // var to handle screenshot index

randomize(); // ensure the map and all other random events are fully randomised - can change this for defined seeds in order to generate proceduralised levels


// create random oases across the level so we have some variety
i=0
for (i=0; i&lt;10; i+=1)
{
    with( instance_create(random_range(16,room_width-16),random_range(16,0.8*room_height) ,choose(obj_Island2,obj_Island3) )){
        while(!place_free(x,y))
        {
            x=random_range(16,room_width-16)
            y=random_range(16,0.8*room_height)
        }
    scaler = random_range(0.6,1)
    image_xscale = scaler
    image_yscale = scaler
    }
    
};

// create random palm trees across the level so we have some variety
i=0
for (i=0; i&lt;100; i+=1)
{

    with( instance_create(random_range(16,room_width-16),random_range(16,room_height-64),obj_Palm1) ){
        while(!place_free(x,y))
        {
            x=random_range(16,room_width-16)
            y=random_range(16,room_height-64)
        }
        image_xscale = choose(1,-1)
    }

}

// create random desserts across the level for collection
global.i=0
for (global.i=0; global.i&lt;global.cakes; global.i+=1)
{
    with( instance_create(random_range(16,room_width-16),(((global.i+1)/(global.cakes+1))*room_height)-(1/10)*room_height,choose(obj_Cake1,obj_Cake2) )){
        while(!place_free(x,y))
        {
            x=random_range(16,room_width-16)
            y=(((global.i+1)/(global.cakes+1))*room_height)-(1/10)*room_height
        }
        image_xscale = choose(1,-1)
      }
 
}

// init player obj and slow animation rate
with (instance_create(room_width/2, room_height-150, obj_MyPlane))    {    image_speed = 0.4;    }

// From Heartbeast book, adjust noise so slightly different each time.
audio_em = audio_emitter_create()
//audio_emitter_pitch(audio_em,random_range(.5,1.5))
//audio_emitter_gain(audio_em,random_range(.7,1))
audio_emitter_gain(audio_em,0.3)
audio_play_sound_on(audio_em,snd_Hover1,true,10)

</string>
