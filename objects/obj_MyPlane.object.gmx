            <string>/// react to player inputs

// Surplant with 'nput' scripts from marketplace which handles keyboard/gamepad simultaneously

if keyboard_check(vk_left){
    image_angle += 2.0
    direction += 2.0
    global.player_dir = image_angle
}    

if keyboard_check(vk_right){
    direction -=2.0
    image_angle -= 2.0
    global.player_dir = image_angle
}

if keyboard_check(vk_up){
    if (speed &lt; 2){
        motion_add(image_angle+90,0.2)
    }
}

if keyboard_check(vk_down){
    if (speed &gt; 0){
        motion_add(image_angle+90,-0.2)
    }
}

// drift to a stop
speed = max(speed - 0.03, 0)



