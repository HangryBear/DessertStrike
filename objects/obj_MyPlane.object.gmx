        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Core Player Code

// Level complete
if global.cakes = 0 { 
    global.cakes = 5
    room_goto_next()
}

// Lose a life
if health &lt;= 0 {
    health = 100
    lives -= 1
    instance_change(obj_Explosion2,true)
}

// Game over
if lives &lt;= 0 {
    // TODO add big explosion sequence
    room_goto(loser_screen)
}

// Turn on enemy radar display for debugging
if (keyboard_check_pressed(ord('R'))){
    if (global.enemyRadar){
            global.enemyRadar = false
        }else {
            global.enemyRadar = true
        }
}


// Surplant with 'nput' scripts from marketplace which handles keyboard/gamepad simultaneously

if keyboard_check(vk_control){
    if can_shoot{
        can_shoot = false
        instance_create(x,y,obj_Player_Bullet)
        alarm[0] = ShootDelay * room_speed
    }
}


if keyboard_check(vk_left) and keyboard_check(vk_shift){

            motion_add(image_angle-180, 0.3*PlayerSpeed);
            speed = clamp(speed,0,0.75*PlayerSpeed)
                    
} else if keyboard_check(vk_left){
    image_angle += 2.0
    direction += 2.0
    global.player_dir = image_angle
}    


if keyboard_check(vk_right) and keyboard_check(vk_shift){
          motion_add(image_angle-180, -0.3*PlayerSpeed);
          speed = clamp(speed,0,0.75*PlayerSpeed)

} else if keyboard_check(vk_right){
    direction -=2.0
    image_angle -= 2.0
    global.player_dir = image_angle
}

if keyboard_check(vk_up){
    if (speed &lt; PlayerSpeed){
        motion_add(image_angle+90,0.2)
    }
}

if keyboard_check(vk_down){
    if speed&lt;=0.5*PlayerSpeed{
       motion_add(image_angle+90,-0.2)
    }
}

// drift to a stop
speed = max(speed - 0.05, 0)

// stop the player from leaving the map
if (x &gt; room_width-16){ 
    x=room_width-16
    speed = 0 
    if !audio_is_playing(snd_Explosion1){
        audio_play_sound(snd_Explosion1, 10, false)
    }
}

if (x &lt; 16){ 
    x = 16
    speed = 0 
    if !audio_is_playing(snd_Explosion1){
        audio_play_sound(snd_Explosion1, 10, false)
    }
}

if (y &gt; room_height-64){ 
   y = room_height-64
   speed = 0
     if !audio_is_playing(snd_Explosion1){
        audio_play_sound(snd_Explosion1, 10, false)
    }
}

if (y &lt; 16){ 
    y = 16
    speed = 0 
    if !audio_is_playing(snd_Explosion1){
        audio_play_sound(snd_Explosion1, 10, false)
    }
}
</string>
